{
  "name": "wedeploy",
  "version": "1.9.0",
  "description": "WeDeploy Api",
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": "wedeploy/api-js",
  "scripts": {
    "format": "prettier --single-quote --no-bracket-spacing --trailing-comma es5 --write \"{src/**/*.js,test/**/*.js,*.js}\" && eslint --fix \"{src/**/*.js,test/**/*.js,*.js}\"",
    "lint": "gulp lint",
    "prepublish": "publish-please guard && gulp build",
    "publish-please": "publish-please",
    "test": "gulp ci"
  },
  "main": "build/node/api-min.js",
  "jsnext:main": "build/es2015/api-min.js",
  "files": [
    "build/es2015/api-min.js",
    "build/globals/api-min.js",
    "build/node/api-min.js"
  ],
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "form-data": "^2.1.2",
    "metal": "^2.0.0",
    "metal-ajax": "^2.0.0",
    "metal-promise": "^2.0.0",
    "metal-storage": "^1.0.2",
    "metal-structs": "^1.0.0",
    "metal-uri": "^2.1.0",
    "request": "^2.67.0",
    "socket.io-client": "^1.7.2"
  },
  "devDependencies": {
    "babel-preset-babili": "0.0.9",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-metal": "^4.1.0",
    "babel-preset-metal-resolve-source": "^1.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-liferay": "^1.0.1",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-metal": "^1.11.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.4.0",
    "isparta": "^4.0.0",
    "metal-tools-build-rollup": "^2.0.0",
    "nock": "^9.0.2",
    "prettier": "^1.2.2",
    "publish-please": "^2.3.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "run-sequence": "^1.1.5",
    "sinon": "^1.17.2",
    "source-map-support": "^0.4.0"
  },
  "react-native": {
    "http": false,
    "request": false
  }
}
